  "scripts": {
    "test": "jest --no-cache",
    "start": "node src/examples.js",
    "build": "npm test && babel src -d lib --minified",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "anime",
    "scrapper"
  ],
  "author": "monkeyandres",
  "license": "GPL-3.0-or-later",
  "dependencies": {
    "@babel/runtime": "^7.4.4",
    "cloudscraper": "^4.1.1",
    "node-html-parser": "^1.1.15",
    "request": "2.88.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "jest": "^24.8.0"
  },
  "files": [
    "lib"
  ],
  "jest": {
    "transformIgnorePatterns": [
      "<rootDir>/src/__test__"
    ]
